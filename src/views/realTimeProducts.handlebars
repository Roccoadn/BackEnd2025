<div id="forms-container">
    <div id="containerAddProduct">
        <form action="http://localhost:8080/api/products" method="POST">
            <input id="productTitle" type="text" placeholder="Nombre del producto" >
            <input id="description" type="text" placeholder="Descripcion del producto">
            <input id="price" type="number" placeholder="Precio del producto">

            <button id="button-addProduct" type="submit">Postear</button>
        </form>
    </div>
    <div id="containerDeleteProduct">
        <form id="delete-Product">
            <input id="input-deleteProduct" type="number" placeholder="ID del producto">
            <button id="button-deleteProduct" type="submit">Borrar</button>
        </form>
    </div>
</div>    
<div id="products-container"></div>

<script src="/socket.io/socket.io.js"></script>
<script src="js/home.js"></script>